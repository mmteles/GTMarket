{
  "name": "gtmarket",
  "version": "1.0.0",
  "description": "GTMarket - AI-powered marketplace application",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "start": "node dist/index.js",
    "netlify:dev": "netlify dev",
    "netlify:deploy": "netlify deploy --prod",
    "vercel-build": "tsc && tsc-alias",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "ai",
    "voice",
    "sop",
    "standard-operating-procedures",
    "speech-to-text",
    "conversation"
