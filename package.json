  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/speech": "^6.7.0",
    "@google-cloud/text-to-speech": "^5.4.0",
    "@google/generative-ai": "^0.24.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.3",
    "chart.js": "^4.4.6",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "docx": "^8.5.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "marked": "^16.0.0",
    "mermaid": "^11.4.0",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.15.0",
    "puppeteer": "^24.0.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.0.3",
    "vercel": "^48.10.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
