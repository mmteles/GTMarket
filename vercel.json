{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/dist/index.js"
    },
    {
      "source": "/",
      "destination": "/public/index-new.html"
    },
    {
      "source": "/new",
      "destination": "/public/index-new.html"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ]
}
