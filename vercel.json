{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/",
      "dest": "/public/index-new.html"
    },
    {
      "src": "/new",
      "dest": "/public/index-new.html"
    },
    {
      "src": "/(.*\\.(css|js|svg|png|jpg|jpeg|gif|ico|html|json))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
